{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "jsRules": {
    "no-empty": true
  },
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "quotemark": [true, "single"],
    "interface-name": [true, "never-prefix"]
    // "no-console": [false, "log", "error"]
  },
  "rulesDirectory": []
}
